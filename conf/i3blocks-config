[cpu_usage2]
command=$SCRIPT_DIR/cpu_usage2/cpu_usage2
markup=pango
interval=persist

[memory]
command=$SCRIPT_DIR/memory/memory
label=|Memory .
interval=5

[rescuetime]
command=$SCRIPT_DIR/rescuetime/rescuetime
interval=30
markup=pango

[volume-pulseaudio]
command=$SCRIPT_DIR/volume-pulseaudio/volume-pulseaudio
LONG_FORMAT="${SYMB} ${VOL}%"
interval=persist
SUBSCRIBE=1
signal=1

[battery]
command=$SCRIPT_DIR/battery2/battery2
interval=30
markup=pango

[time]
command=$SCRIPT_DIR/time/time
interval=1
STRFTIME_FORMAT=%y-%m-%d %H:%M
TZONES=Europe/Nicosia
TZ_LABELS=
